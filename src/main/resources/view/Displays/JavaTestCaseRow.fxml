<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.HBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="50.0" prefWidth="500.0" spacing="15.0" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextArea fx:id="inputTextArea" prefHeight="48.0" prefWidth="204.0"/>
        <TextArea fx:id="outputTextArea" prefHeight="48.0" prefWidth="210.0"/>
        <Button fx:id="addRowButton" mnemonicParsing="false" prefHeight="42.0" prefWidth="52.0" text="Add more"
                textAlignment="CENTER" wrapText="true"/>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="2.0"/>
    </padding>
</fx:root>
